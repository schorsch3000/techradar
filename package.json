    "jasmine": "istanbul cover jasmine",
    "flow": "flow",
    "shrinkpack": "rm -rf ./node_modules && rm -f npm-shrinkwrap.json && npm i && npm shrinkwrap --dev && shrinkpack"
  },
  "author": "Thomas Franz",
  "repository": {
    "type": "git",
